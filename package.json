{
  "name": "openllmprovider",
  "version": "0.1.0",
  "description": "Control plane for AI model providers",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./auth": {
      "import": {
        "types": "./dist/auth/index.d.mts",
        "default": "./dist/auth/index.mjs"
      },
      "require": {
        "types": "./dist/auth/index.d.cts",
        "default": "./dist/auth/index.cjs"
      }
    },
    "./storage": {
      "import": {
        "types": "./dist/storage/index.d.mts",
        "default": "./dist/storage/index.mjs"
      },
      "require": {
        "types": "./dist/storage/index.d.cts",
        "default": "./dist/storage/index.cjs"
      }
    },
    "./plugin": {
      "import": {
        "types": "./dist/plugin/index.d.mts",
        "default": "./dist/plugin/index.mjs"
      },
      "require": {
        "types": "./dist/plugin/index.d.cts",
        "default": "./dist/plugin/index.cjs"
      }
    }
  },
  "files": ["dist", "src", "README.md"],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "keywords": ["ai", "llm", "provider", "registry", "control-plane"],
  "author": "Chatbox Team",
  "license": "MIT",
  "peerDependencies": {
    "@ai-sdk/amazon-bedrock": "^4.0.0",
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/azure": "^3.0.0",
    "@ai-sdk/google": "^3.0.0",
    "@ai-sdk/google-vertex": "^4.0.0",
    "@ai-sdk/groq": "^3.0.0",
    "@ai-sdk/mistral": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "@ai-sdk/openai-compatible": "^2.0.0",
    "@ai-sdk/xai": "^3.0.0",
    "@openrouter/ai-sdk-provider": "^2.2.0",
    "zod": ">=3.20.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/amazon-bedrock": {
      "optional": true
    },
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/azure": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/google-vertex": {
      "optional": true
    },
    "@ai-sdk/groq": {
      "optional": true
    },
    "@ai-sdk/mistral": {
      "optional": true
    },
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/openai-compatible": {
      "optional": true
    },
    "@ai-sdk/xai": {
      "optional": true
    },
    "@openrouter/ai-sdk-provider": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.3.0"
  },
  "devDependencies": {
    "@ai-sdk/amazon-bedrock": "^4.0.0",
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/azure": "^3.0.0",
    "@ai-sdk/google": "^3.0.0",
    "@ai-sdk/google-vertex": "^4.0.0",
    "@ai-sdk/groq": "^3.0.0",
    "@ai-sdk/mistral": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "@ai-sdk/openai-compatible": "^2.0.0",
    "@ai-sdk/provider": "^3.0.0",
    "@ai-sdk/xai": "^3.0.0",
    "@biomejs/biome": "^1.9.0",
    "@openrouter/ai-sdk-provider": "^2.2.0",
    "@types/bun": "^1.3.9",
    "@types/debug": "^4.1.0",
    "@types/node": "^25.3.0",
    "tsdown": "^0.20.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.0",
    "zod": "^3.23.0"
  }
}
